page {
    background: #efefef;
  }
  /* pages/fenlei/fenlei.wxss */
  
  .top {
    background: white;
    display: flex;
    justify-content: space-around;
    align-items: center;
    padding-bottom: 20rpx;
  }
  
  .top icon {
    display: flex;
    align-items: center;
  }
  .left{
    width: 33.33%;
  }
  .center {
    width: 33.33%;
    display: flex;
    justify-content: space-around;
    align-items: center;
    background: white;
    border-radius: 10rpx;
    padding: 0 20rpx;
  }
  .right{
    width: 33.33%;
    text-align: right;
    padding-right: 10rpx;
  }
  .guanli{
    color:#037D6E;
  }
  .iconfont {
    color: black;
    font-size: 45rpx;
  }
  
  
  
  
  .cartsselect {
    margin-top: 10rpx;
    margin-right: 10rpx;
    width: 39rpx;
    height: 39rpx;
  }
  
  .history {
    background: white;
    margin-bottom: 30rpx;
    border-radius: 2rem;
    width: 100%;
  }
  
  .price {
    color: #f60;
  }
  
  .num {
    color: #989699;
  }
  
  .historycnt {
    font-size: 15px;
    display: flex;
    justify-content: space-between;
  }
  
  .historysubtitle {
    align-items: flex-end;
    display: flex;
    flex-direction: column;
    font-size: 15px;
  }
  
  .historytitle {
    margin: 0 auto;
    word-break: break-all;
    width: 350rpx;
    font-size: 23rpx;
  }
  
  
  .historylist {
    padding: 0 20rpx;
  }
  
  .historylist .historyitem {
    border-radius: 2rpx;
    border-bottom: 1rpx solid #e3e3e3;
    box-sizing: border-box;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 20rpx 10rpx;
    width: 100%;
    position: relative;
  }
  
  .historyitem:nth-child(1) {
    border-top: 0;
  }
  
  .historyitem .historycnt {
    flex: 1;
    padding: 0 20rpx;
  }
  
  .historycnt .historyimage {
    float: left;
    margin-right: 20rpx;
    width: 164rpx;
    height: 164rpx;
  }
  
  .historyinfo {
    flex: 1;
    display: flex;
    justify-content: flex-start;
    justify-content: space-around;
  }
  
  .historytitle {
    font-size: 25rpx;
  }
  
  .historybutton {
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: flex-end;
  }
  
  .historytotal {
    padding-top: 30rpx;
    padding-bottom: 30rpx;
    margin-right: 36rpx;
  }
  
  .button {
    margin-right: 12rpx;
    padding-top: 30rpx;
    padding-bottom: 30rpx;
    display: flex;
    align-items: center;
    justify-content: space-around;
  }
  
  .button view {
    margin-right: 12rpx;
    width: 150rpx;
    text-align: center;
    border-radius: 2rem;
  }
  
  .button view text {
    font-size: 24rpx;
    line-height: 80rpx;
  }
  
  .buttonown {
    border: 1rpx solid #A9A6AA;
    border-radius: 4rpx;
    background: white;
  }
  
  .buttonown text {
    color: #7A777B;
  }
  
  .buttontwo {
    border: 1rpx solid #037D6E;
    border-radius: 4rpx;
    background: #037D6E;
  }
  
  .buttontwo text {
    color: white;
  }
  
