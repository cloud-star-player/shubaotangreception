/*foot使用*/
.footer{
  height: 85rpx;
  width: 100%;
  display: flex;
	flex-flow: row wrap;
	justify-content: space-around;
	align-items: center;
	position:fixed;bottom:0;left:0;
  z-index: 9999;
  background: white;
}
.footer view{
  width: 33.3%;
}
.footer-buffer{
  height: 85rpx;
  width: 100%;
}
.footers{
  width: 33.3%;
  display: flex;
	flex-flow: row wrap;
	justify-content: space-around;
}

.footers view{
  
  width: 50%;
  display: flex;
  flex-direction: column;
  text-align: center;
  padding-bottom: 20rpx;
}
.footers view icon{
 color:  #9f9f9f;
 font-size: 40rpx;
}
.footers view text{
 color:  #9f9f9f;
 font-size: 20rpx;
}
.shopping{
  background: #f29901;
  color: white;
  text-align: center;
  padding-top:20rpx; 
  height: 85rpx;
}
.purchase{
  background: #fe4543;
  padding-top:20rpx; 
  color: white;
  text-align: center;
  height: 85rpx;
}

