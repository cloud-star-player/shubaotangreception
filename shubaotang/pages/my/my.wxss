  border-bottom: 20rpx solid #eee;
} */

.bottom {
  margin-top:50rpx; 
  display: flex;
  flex-flow: row wrap;
  justify-content: space-around;
  width: 100%;
  align-items: center;
}

.bottom view {
  align-items: center;
  display: flex;
  flex-flow: column wrap;
}

.bottom view view icon {
  font-size: 54rpx;
  color: white;
}

.bottom view view {
  margin-bottom: 25rpx;
  box-shadow: 0.1875rem 0.1875rem 0.3125rem #333;
  width: 86rpx;
  height: 86rpx;
  border-radius: 5px;
  /*只需要在父元素上加这三句*/
  justify-content: center; /*子元素水平居中*/
  align-items: center; /*子元素垂直居中*/
  display: -webkit-flex;
}

.b1 {
  background: #ef5b2b;
}
.b2 {
  background: #ef274c;
}
.b3 {
  background: #f1ac2b;
}
.b4 {
  background: #a7bd2c;
}
.b5 {
  background: #f27197;
}
.bottom view text {
  font-size: 26rpx;
}

