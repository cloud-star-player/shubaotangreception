/* pages/my/my.wxss */
.top {
  width: 100%; 
  height: 216rpx;  
  border-color: #b9cbcc transparent transparent transparent;
  border-radius: 0 0 10px 10px ;
  background: #b9cbcc;
  margin-bottom: 135rpx;
}
page{
  background: #efefef;
}
.card{
  z-index: 999;
  margin: auto;
  position:absolute;
  left:0;
  right: 0;
  top:60rpx;
  border-radius:5rem; 
  /*微调*/  
  width: 90%;
  padding: 69rpx 0;
  background: #fff;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.card_user image{
  width: 97rpx;
  height: 97rpx;
  border-radius:50%;
  display: flex;
  align-items: center;
  flex-flow: row-wrap;
  margin-right:30rpx; 
  box-shadow: 0.1875rem 0.1875rem 0.3125rem #333;
}
.card_user{
  margin-left:30rpx; 
  display: flex;
  align-items: center;
  flex-flow: row-wrap;
}
.member{
  border-radius:50px; 
  width: 190rpx;
  height: 68rpx;
  margin-right:30rpx; 
  display: flex;
  align-items: center;
  flex-flow: row-wrap;
  background: #4d515a;
  justify-content: space-between;
}
.member icon{
  margin-left:10rpx; 
  font-size: 50rpx;
  color: #fff;
}
.member text{
  margin-right:10rpx; 
  color: #fff;
  font-size: 20rpx;
  display: flex;
  align-items: center;
  margin-bottom:20rpx; 
}

.my_order{
  background: white;
  padding-bottom: 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 20rpx solid #eee;
}

.my_order_text1 {
  margin-left: 24rpx;
  color: #616161;
}

.my_order_text2{
  margin-right: 24rpx;
  color: #9c9c9c;
  font-size: 28rpx;
}
.my_list{
  width:100%;
  background: white;
  margin-top: 40rpx;
  padding-top: 40rpx;
  padding-bottom: 40rpx;
  display: flex;
  flex-flow: row wrap;
  justify-content: space-around;
  border-bottom: 20rpx solid #eee;
}
.my_list view{
  flex: 1;
  display: flex;
  flex-flow: column wrap;
  align-items: center;
}
.my_list view icon{
  color: #989898;
  font-size: 54rpx;
}
.my_list view text{
  color: #989898;
  font-size: 25rpx;
}
